<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hibernatedao.gymtracking.Activity" table="ACTIVITY">
        <id name="activityId" type="int">
            <column name="ACTIVITYID" />
            <generator class="increment" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" />
        </property>
        <property name="details" type="java.lang.String">
            <column name="DETAILS" />
        </property>
        <property name="set" type="int">
            <column name="SET" />
        </property>
        <property name="reps" type="int">
            <column name="REPS" />
        </property>
    </class>
</hibernate-mapping>
